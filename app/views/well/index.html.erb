
<table>
  <tr>
    <th>TO</th>
    
    <th>MESSAGE</th>

    <th>FILE</th>
  </tr>
  
<% @body.each do |c| %>
<tr>
<td><%= c.to %></td>

<td><%= c.message[1.10].html_safe %></td>
<td><%= c.image %></td>
<td><%= link_to"view",well_path(c) %></td>

<% end %>
</tr>
</table>
<%= link_to 'Back',  new_well_path %> 
