<table>
  <tr>
  
    <th>FROM</th>
    
    <th>MESSAGE</th>

    <th>FILE</th>

  </tr>
  
<% @messages.each do |c| %>
<tr>

<td><%= c.user_id %></td>

<td><%= c.body[1.10].html_safe %></td>
 <td><%=  c.image %></td>

<td><%= link_to"view",well_path(c) %></td>
<td><%= link_to "delete",well_path(c) , method: :delete, data: { confirm: 'Are you sure?' } %></td>
<% end %>
</tr>
</table>
<%= link_to 'Back',  new_well_path %> 
